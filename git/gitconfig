# This is Git's per-user configuration file.
[color]
	branch = auto
	diff = auto
	status = auto
[user]
	name = Vivitsu Maharaja
	email = vivitsu.maharaja@gmail.com
[core]
	editor = emacsclient -c -a emacs
	excludesfile = ~/.gitexcludes
[push]
	default = simple
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[alias]
	st = status
	ci = commit
	co = checkout
	br = branch

